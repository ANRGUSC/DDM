{
  "name": "webcrypto-shim",
  "main": "webcrypto-shim.js",
  "version": "0.1.1",
  "description": "Web Cryptography API shim for legacy browsers",
  "license": "MIT",
  "authors": [
    "Artem S Vybornov <vybornov@gmail.com>"
